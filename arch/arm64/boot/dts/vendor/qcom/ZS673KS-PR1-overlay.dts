&qupv3_se0_i2c {
	ms51@16 {
		nuvoton,hw_stage = <5>;
	};
};

//ASUS_BSP: NFC +++
&qupv3_se15_i2c{
	nq@28{
	    status = "ok";
	    qcom,sn-ven = <&tlmm 66 0x00>;
	};
};
//ASUS_BSP: NFC ---

//ASUS_BSP: usb redriver +++
&usb1 {
	vcc_redriver-supply = <&pm8008j_l6>;
	REDRIVER_RSTN = <&tlmm 3 0x0>;
	REDRIVER_EQ_EN = <&tlmm 62 0x0>;
};
//ASUS_BSP: usb redriver ---

