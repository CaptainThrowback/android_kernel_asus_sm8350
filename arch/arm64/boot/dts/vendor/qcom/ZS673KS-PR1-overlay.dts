//ASUS_BSP: NFC +++
&qupv3_se15_i2c{
	nq@28{
	    status = "ok";
	    qcom,sn-ven = <&tlmm 66 0x00>;
	};
};
//ASUS_BSP: NFC ---

/*    ====================================
 *    overlay for lahaina-mtp.dtsi - end
 *    ====================================
